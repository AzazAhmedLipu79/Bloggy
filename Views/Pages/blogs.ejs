<article >
    <h2
      class="text-center text-3xl hover:underline bg-primary text-white font-bold px-10 py-10">
      Blog Section
    </h2>
  
    <div class="grid md:grid-cols-3 lg:grid-cols-4 gap-4 ">
        <% for (const blog of blogs) { %>
          <div class="bg-slate-900 shadow-md border border-gray-200 rounded-lg max-w-[400px] mb-5 mx-auto">
            <a href="#">
              <img class="rounded-t-lg" src="https://flowbite.com/docs/images/blog/image-1.jpg" alt="" />
            </a>
            <div class="p-5">
              <a href="#">
                <h5 class="text-gray-100 font-bold text-2xl tracking-tight mb-2">
                  <%= blog.title %>
                </h5>
              </a>
              <p class="font-normal text-gray-300 mb-3"><%= blog.content %></p>
              <button  
                class="text-white bg-indigo-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-3 py-2 text-center inline-flex items-center"
                onclick={blog_navigating(`<%= blog.id %>`)}
              >
          Liked By  <%= blog.thumbsup %>
              </button>
            </div>
          </div>
        <% } %>
      </div>
  </section>
  <script>
    
 const   blog_navigating=(a)=>{ 
    console.log(a)
  routemamba.navigate(`/blogs/${a}`);}
   
  </script>
  
